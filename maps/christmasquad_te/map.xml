    <contributor uuid="dad8b95c-cf6a-44df-982e-8c8dd70201e0" contribution="Kits and Ideas (From Cannon Duel)"/> <!-- ElectroidFilms -->
</contributors>
<spawns>
    <spawn team="red-team" region="red-spawn" kit="red" yaw="90"/>
    <spawn team="blue-team" region="blue-spawn" kit="blue" yaw="180"/>
    <spawn team="green-team" region="green-spawn" kit="green" yaw="0"/>
    <spawn team="yellow-team" region="yellow-spawn" kit="yellow" yaw="-90"/>
    <default region="obs-spawn" yaw="0" pitch="20"/>
</spawns>
<time>10m</time>
<blitz>
    <lives>1</lives>
</blitz>
<teams>
    <team id="red-team" color="dark red" max="4" max-overfill="4">Red</team>
    <team id="blue-team" color="blue" max="4" max-overfill="4">Blue</team>
    <team id="green-team" color="green" max="4" max-overfill="4">Green</team>
    <team id="yellow-team" color="yellow" max="4" max-overfill="4">Yellow</team>
</teams>
