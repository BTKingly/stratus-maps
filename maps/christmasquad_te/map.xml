<timelock>on</timelock>
<maxbuildheight>25</maxbuildheight>
<hunger>
    <depletion>off</depletion>
</hunger>
<destroyables>
    <destroyable id="Placeholder" name="Placeholder" region="connected-bridges" materials="stained glass" completion="0%" show="false" mode-changes="true" owner="blue-team"/>
</destroyables>
<modes>
    <mode after="20s" material="air" show-before="20" name="Fight!"/>
    <mode after="4m" material="95:15" show-before="120" name="Connecting the platforms."/>
    <mode after="7m30s" material="95:15" show-before="120" name="Border shrink begins."/>
</modes>
<world-borders center="-12.5,-38.5">
    <world-border size="125"/>
    <world-border size="10" after="7m" duration="3m"/>
</world-borders>
</map>