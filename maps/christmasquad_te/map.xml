<filters>
    <not id="deny-glass-world">
        <all>
            <any>
                <material>160</material>
                <material>95</material>
                <material>chest</material>
            </any>
            <any>
                <cause>world</cause>
                <not>
                    <cause>player</cause>
                </not>
            </any>
        </all>
    </not>
    <not id="deny-glass-break">
        <any>
            <material>160</material>
            <material>95</material>
            <material>chest</material>
        </any>
    </not>
    <all id="region-enabled">
        <time>20s</time>
    </all>
</filters>
<regions>
    <!-- Region Definitions -->
    <negative id="not-playable">
        <rectangle id="playable" min="34,8" max="-60,-86"/>
    </negative>
    <cuboid id="top" min="oo,51,oo" max="-oo,oo,-oo"/>
    <above id="no-break" y="25"/>
    <union id="no-bridge-region">
        <rectangle id="blue-red" min="34.0,-32.0" max="7,-45"/>
        <rectangle id="red-green" min="-6,-86" max="-19,-58"/>
        <rectangle id="green-yellow" min="-60,-45" max="-32,-32"/>
        <rectangle id="yellow-blue" min="-19,8" max="-6,-19"/>
    </union>
    <union id="center-bridging-areas">
        <rectangle min="-19.0,-20.0" max="-6,-57.0"/>
        <rectangle min="6.0,-32.0" max="-31.0,-45.0"/>
    </union>
    <!-- Spawns -->
    <cylinder id="red-spawn" base="24.5,61,-75.5" radius="3" height="0"/>
    <cylinder id="blue-spawn" base="24.5,61,-1.5" radius="3" height="0"/>
    <cylinder id="green-spawn" base="-49.5,61,-75.5" radius="3" height="0"/>
    <cylinder id="yellow-spawn" base="-49.5,61,-1.5" radius="3" height="0"/>
    <cylinder id="obs-spawn" base="-12.5,37,-38.5" radius="1" height="0"/>
    <cuboid id="connected-bridges" min="100,63,100" max="-100,6,-100"/>
    <cuboid id="death-kit-region" min="-18,31,-44" max="-8,50,-34"/>
    <!-- Applied Rules -->
    <apply block="never" region="no-bridge-region" message="You may not bridge outside your area."/>
    <apply kit="death" region="death-kit-region"/>
    <apply block="never" region="top" message="You may not build here."/>
    <apply block="never" region="not-playable" message="You may not build outside the playable area."/>
    <apply block-break="deny-glass-world" region="everywhere"/>
    <apply block-break="deny-glass-break" message="You may not break this block." region="everywhere"/>
    <apply block="region-enabled" region="center-bridging-areas" message="This region is disabled until 7m 30s has passed."/>
</regions>
<disabledamage>
    <damage ally="true" self="false" enemy="false" other="false">block explosion</damage>
</disabledamage>
<broadcasts>
    <alert after="1s">`7You have 20 seconds to prepare.</alert>
    <tip after="10s">`7After 4 minutes the bridges will connect (You cannot bridge until then).</tip>
    <alert after="15s">`7TNT is hidden underneath the islands, so be careful or you may be destroyed.</alert>
    <tip after="20s">After 2 kills, you are automatically given an iCarus blade. (You must have inventory space).</tip>
    <alert after="7m30s">You may now build in the center.</alert>
</broadcasts>
